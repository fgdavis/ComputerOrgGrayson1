rjpublic class treefill {
        
    public static void main(String[] args) {
    	//Node being the assumed name of the class
        Node bt = new Node(arr[0]);
        for(int i = 1;i<arr.length;i++){
        	bt.insert(arr[i]);
        }
    }
    
   //goes where the tree is
		
	public void insert(int num) { 
		data = addNode(data, num); 
    	} 
      
   	public Node addNode(Node data, int num) { 
        	if (data.left == null) { 
            	data.left = new Node(num); 
            	return data; 
        	} 
  
        	else{ 
            		data.right = new Node(num); 
	    	}
  
        	return data; 
    	} 
}
